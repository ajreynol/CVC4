(include "../theories/BitVectors.eo")
(include "../theories/Ints.eo")

(declare-const int_to_bv (->
  (! Int :var w)
  Int (BitVec w))
)

(declare-const ubv_to_int (->
  (! Int :var m :implicit)
  (BitVec m) Int)
)

(declare-const sbv_to_int (->
  (! Int :var m :implicit)
  (BitVec m) Int)
)

; disclaimer: >
;   This function is not a function in SMT-LIB, it is a synonym of the SMT-LIB
;   standard operator int_to_bv.
(define int2bv () int_to_bv)

; disclaimer: >
;   This function is not a function in SMT-LIB. Although it is used
;   to describe semantics of bv operators, it is not a theory symbol.
(declare-const bv2nat (->
  (! Int :var m :implicit)
  (BitVec m) Int)
)
