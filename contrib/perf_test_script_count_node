./perf_test_script 2 12 3 $@
./perf_test_script 2 12 4 $@
./perf_test_script 2 12 5 $@
./perf_test_script 2 12 6 $@
./perf_test_script 2 12 7 $@
./perf_test_script 2 12 8 $@
./perf_test_script 2 12 9 $@
./perf_test_script 2 12 10 $@
./perf_test_script 2 12 11 $@
./perf_test_script 2 12 12 $@
